    <http:listener-config name="HTTP_Listener_Configuration" host="0.0.0.0" port="9090" doc:name="HTTP Listener Configuration"/>
    <flow name="jmsFlow">
        <http:listener config-ref="HTTP_Listener_Configuration" path="/jms-sender" allowedMethods="GET" doc:name="HTTP"/>
        <set-payload value="#[message.inboundProperties.'http.query.params'.data]" doc:name="Set Payload"/>
        <jms:outbound-endpoint doc:name="JMS OBEP"/>
    </flow>
